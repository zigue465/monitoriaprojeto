#include <stdio.h>
#include <stdlib.h>

int main(){
   int ano_nasc, ano_atual, idade;

   printf("digite o seu ano de nascimento: ");
   scanf("%d", &ano_nasc);

   printf("digite em que ano estamos: ");
   scanf("%d", &ano_atual);

   idade = ano_atual - ano_nasc;

   if (idade>=16){
      printf("voce ja pode votar. ");
   } else {
       printf("voce ainda nao pode votar, sua idade eh de %d anos", idade);
   }



    return 0;
}
